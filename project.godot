; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Twenty-Eight"
run/main_scene="res://title_menu.tscn"
config/features=PackedStringArray("4.2", "Mobile")

[autoload]

Music="*res://music.tscn"
Global="*res://Scripts/Globals.gd"

[display]

window/stretch/mode="viewport"
mouse_cursor/custom_image="res://Assets/4 cursor.png"
mouse_cursor/custom_image_hotspot=Vector2(10, 4)

[editor]

movie_writer/mjpeg_quality=0.5
movie_writer/movie_file="C:/Users/User/Desktop/28-clip.avi"
movie_writer/fps=30
version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/android/enable_long_press_as_right_click=true

[rendering]

renderer/rendering_method="mobile"
